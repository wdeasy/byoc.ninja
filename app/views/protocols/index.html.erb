<% provide(:title, "Protocols List")  %>
<div class="jumbotron">
  <h3 class="center">GameQ Protocols</h3>

  <table class="table table-striped table-condensed games">
    <thead>
      <tr class="row">
        <th class="col-md-6 text-left">Name</th>
        <th class="col-md-6 text-right">Protocol</th>
      </tr>
    </thead>
    <tbody>
      <% if @protocols.empty? %>
        <tr class="row"><td colspan="3" class="col-md-offset-2 text-center">No Protocols Found.</td></tr>
      <% else %>
        <%= render @protocols %>
      <% end %>              
    </tbody>
  </table>
</div>