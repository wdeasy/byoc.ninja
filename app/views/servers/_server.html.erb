<tr>
  <td class="text-left longtext" title="<%= server.game.gameextrainfo %>">
    <% if server.game.gameid.length < 7 %>
      <%= store_link server.game, server.game.gameextrainfo, {:target => "_blank"} %>
    <% else %>
      <%= server.game.gameextrainfo %>
    <% end %> 
  </td>
  <td class="text-center longtext">
    <%= flags(server).html_safe %>
  </td>
  <td class="text-left longtext" title="<%= server.name %>">
    <%= server.name %>
  </td>
  <td class="text-right">
    <%= players(server).html_safe %>
  </td>
  <td class="text-center">
    <div class="dropdown">
    <%= link_to server.users.size, '#', :class => 'dropdown-toggle', :'data-toggle' => 'dropdown' %>
    <ul class="dropdown-menu">
      <% server.users.each do |user| %>
        <li><%= link_to display_name(user), user.profileurl, :target => '_blank' %></li>
      <% end %>
    </ul>
    </div>
  </td>
  <td class="text-left longtext" title="<%= server.map %>">
    <%= server.map %>
  </td>
  <td class="text-right">
    <% if !server.gameserverip.include? "." %>
      <%= link_to "Join Lobby", "steam://joinlobby/#{server.game.gameid}/#{server.lobbysteamid}/#{server.users.first.steamid}" %>
    <% elsif server.lobbysteamid != nil %>
      <%= link_to server.gameserverip, "steam://joinlobby/#{server.game.gameid}/#{server.lobbysteamid}/#{server.users.first.steamid}" %>
    <% else %>
      <%= link_to server.gameserverip, "steam://connect/#{server.gameserverip}" %>
    <% end %>
  </td>
</tr>