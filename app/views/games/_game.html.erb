  <div class="row">
  <div class="col-md-3"></div>
    	<div class="col-md-6">
      <div class="row">
        <% if logged_in? && current_user.admin? %>
          [<%= link_to "edit", edit_game_path(game)%>]
        <% end %>
	      <% if game.url %>
	      	<%= link_to game.name, game.url %>
        <% else %>
	      	<%= game.name %>
	      <% end %>
        <% if game.joinable == false%>
          [unable to join]
        <% end %>
        </div>
    	</div>
    <div class="col-md-3"></div>
  </div>
